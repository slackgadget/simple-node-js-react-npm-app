{
  "extends": "airbnb",
  "root": true,
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-underscore-dangle":"off",
    "strict": "off",
    "max-len": [
      "error",
      200
    ],
    "react/prop-types": "off",
    "react/prefer-stateless-function": [
      0
    ],
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ],
    "react/jsx-props-no-spreading": "off",
    "no-param-reassign": [2, {"props": false}]
  }
}
